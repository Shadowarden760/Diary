CREATE TABLE Note (
    noteId INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    noteCategory TEXT NOT NULL,
    noteTitle TEXT NOT NULL,
    noteMessage TEXT NOT NULL
);

getAllNotes:
SELECT Note.*
FROM Note;

getNoteById:
SELECT Note.*
FROM Note
WHERE Note.noteId = ?;

insertNote:
INSERT INTO Note(noteId, noteCategory, noteTitle, noteMessage)
VALUES (?, ?, ?, ?);

deleteNoteById:
DELETE FROM Note
WHERE Note.noteId = ?;

lastInsertedId:
SELECT last_insert_rowid();